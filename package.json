{
  "name": "web-sequencer",
  "description": "Simple Web Sequencer App",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.dev.js --port:3000",
    "test": "jest --watchAll",
    "build": "webpack --config webpack.config.prod.js"
  },
  "dependencies": {
    "immer": "2.1.3",
    "midi-sounds-react": "^1.2.54",
    "prop-types": "15.7.2",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-keyboard-event-handler": "^1.5.4",
    "react-toastify": "^6.0.5",
    "reselect": "4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "autoprefixer": "^9.8.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-preset-react-app": "7.0.2",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "2.1.1",
    "cssnano": "4.1.10",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.11.2",
    "eslint": "5.15.2",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.9.0",
    "jest": "24.5.0",
    "json-server": "0.14.2",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "4.1.5",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-sort-media-queries": "^1.5.24",
    "react-redux": "^7.2.0",
    "react-styleable": "^3.3.2",
    "react-test-renderer": "16.8.4",
    "react-testing-library": "6.0.0",
    "redux": "^4.0.5",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-thunk": "^2.3.0",
    "rimraf": "2.6.3",
    "sass-loader": "^8.0.2",
    "style-loader": "0.23.1",
    "webpack": "4.29.6",
    "webpack-bundle-analyzer": "3.1.0",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.2.1"
  },
  "engines": {
    "node": ">=8"
  },
  "babel": {
    "presets": [
      "babel-preset-react-app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "no-debugger": "off",
      "no-console": "off",
      "no-unused-vars": "warn",
      "react/prop-types": "warn"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "root": true
  }
}
